{
  "manifest_version": 2,
  "name": "Pointbreak",
  "version": "1.0",
  "app": {
    "background": {
      "scripts": ["app.js"]
    }
  },
  "icons": {
      "16": "icons/pointbreak-16.png",
      "48": "icons/pointbreak-48.png",
      "128": "icons/pointbreak-128.png" 
  },
  "permissions": [
    "webview",
    "storage"
  ]
}
